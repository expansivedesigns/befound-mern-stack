{
  "name": "befound-backend",
  "version": "1.0.0",
  "description": "Befound Database Backend",
  "main": "server.js",
  "scripts": {
    "build": "cd client && npm run build",
    "install-client": "cd client && npm install",
    "heroku-postbuild": "npm run install-client && npm run build",
    "start": "nodemon server.js",
    "client": "cd client && npm start",
    "dev": "concurrently -n 'server,client' -c 'red,green'\"nodemon server.js\" \"npm run client\""
  },
  "keywords": [
    "database",
    "backend",
    "rest",
    "api",
    "web",
    "services"
  ],
  "author": "TaLoveous Goodwin",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "concurrently": "^6.0.0",
    "config": "^3.3.6",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "create-react-app": "^4.0.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "js-cookie": "^2.2.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.11.15",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.7",
    "serve": "^11.3.2",
    "uninstall": "0.0.0"
  },
  "engines": {
    "node": "15.0.1"
  }
}
